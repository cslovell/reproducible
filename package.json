{
  "name": "reproducible-button-extension",
  "version": "0.1.1",
  "description": "Quarto extension for generating reproducible environment buttons with Onyxia deep-links",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:bash": "bash tests/bash/test.sh",
    "test:all": "npm run test:bash && npm run test"
  },
  "keywords": [
    "quarto",
    "extension",
    "reproducible",
    "onyxia",
    "jupyter"
  ],
  "author": "UN Handbook Team",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
